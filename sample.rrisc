;insert key into cache
exec victim 

;initialize looping condition
set 0xff, R1 
set 0x1, R2 

;start looping through the cache
label start_loop 

;flush and reload
flush R1 
exec p0 
clocktime R3 
load R1 
clocktime R4 
sub R3, R4 
set 0x1, R3 
sub R3, R4 
jumpifzero R4, end_loop 
sub R2, R1 
jumpifzero R1, start_loop 
label end_loop
check R1 
